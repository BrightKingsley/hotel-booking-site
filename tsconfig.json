{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "allowJs": false,
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@api/*": [
        "./src/api/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      // "@allowSyntheticDefaultImports/*": [
      //   "./src/allowSyntheticDefaultImports/*"
      // ],
      "@constants/*": [
        "./src/constants/*"
      ],
      "@context/*": [
        "./src/context/*"
      ],
      "@data/*": [
        "./src/data/*"
      ],
      "@hooks/*": [
        "./src/hooks/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@routes/*": [
        "./src/routes/*"
      ]
    },
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src",
    "public/prev"
  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}